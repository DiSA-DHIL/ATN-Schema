                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Ukrainian</a:documentation>
                           <value>ur</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Urdu</a:documentation>
                           <value>uz</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Uzbek</a:documentation>
                           <value>vi</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Vietnamese</a:documentation>
                           <value>wo</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Wolof</a:documentation>
                           <value>xh</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Xhosa</a:documentation>
                           <value>yo</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Yoruba</a:documentation>
                           <value>zh</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chinese</a:documentation>
                           <value>zu</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Zulu</a:documentation>
                           <value>la</value>
                           <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Latin</a:documentation>
                        </choice>
